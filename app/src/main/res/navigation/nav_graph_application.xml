<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:id="@+id/nav_graph_application"
	app:startDestination="@id/signUpFragment">

	<fragment
		android:id="@+id/signUpFragment"
		android:name="com.example.shiftlab2022.screens.SignUpFragment"
		android:label="SignUpFragment" >
		<action
			android:id="@+id/action_signUpFragment_to_welcomeFragment"
			app:destination="@id/welcomeFragment"
			app:popUpTo="@navigation/nav_graph_application"
			app:popUpToInclusive="true"/>
	</fragment>
	<fragment
		android:id="@+id/welcomeFragment"
		android:name="com.example.shiftlab2022.screens.WelcomeFragment"
		android:label="WelcomeFragment" />
</navigation>